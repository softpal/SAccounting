            <div class="plan">
                <h2>Tiny Business</h2>
                <div class="pricing-info">
                    <div class="plan-subhead">
                        for Business
                    </div>
                    <span>$ 15/Annually</span><br>
                    1 admin + 1 user account.
                    <br>
                    MO/$15 billed annually
                    <br>
                    <br>
                </div>
                <div class="list-text">
                    <div class="signup-giant">
                         <a class="btn1 btn-success1" href="~/Registration/SignUp/?licience=2">Sign Up</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="princing-t pricing hover-effect">
            <div class="plan highlight">
                <h2>Less than 5 Employees</h2>
                <div class="pricing-info">
                    <div class="plan-subhead">
                        for Employees
                    </div>
                    <span>$ 14/Annually</span>
                    <br>
                    1 admin + 5 user account
                    <br>
                    MO/$14 billed annually
                    <br>
                    <br>
                </div>
                <div class="list-text">
                    <div class="signup-giant">
                        <a class="btn1 btn-success1" href="~/Registration/SignUp/?licience=6">Sign Up</a>
                    </div>
                </div>
            </div>


        </div>
        <div class="princing-t pricing hover-effect">
            <div class="plan">
                <h2>More than 6 Employees</h2>
                <div class="pricing-info">
                    <div class="plan-subhead">
                        for Employees
                    </div>
                    <span>$ 13/Annually</span><br>
                    Includes 1 admin account<br>
                    MO/$13 billed annually
                    <br>
                    <br>
                </div>
                <div class="list-text">
                    <div class="signup-giant">
                        <a class="btn1 btn-success1" href="~/Registration/SignUp/?licience=14">Sign Up</a>
                    </div>
                </div>
            </div>
        </div>
    </div>

