@model StratusAccounting.BAL.LoggedUser

@{
    ViewBag.Title = "PasswordChange";
    Layout = "~/Views/Shared/_SuperAdminView.cshtml";
}

@Html.BeginForm()
{

<div class="page-content">
    <!-- BEGIN PAGE CONTAINER-->
    <div class="container-fluid">
        <!-- BEGIN PAGE HEADER-->
        <div class="row-fluid">
            <div class="span12">

                <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                <!--<h3 class="page-title">Preferences</h3>-->
                <ul class="breadcrumb margin-top-20">
                    <li><i class="icon-home"></i><a href="#">Home</a> <i class="icon-angle-right"></i></li>

                    <li><a href="sa_account.html">My Account</a></li>
                </ul>
                <!-- END PAGE TITLE & BREADCRUMB-->
            </div>
        </div>
        <!-- END PAGE HEADER-->
        <!-- BEGIN PAGE CONTENT-->
        <div class="row-fluid">
            @Html.ValidationSummary()
            <div class="span12">
                <!-- BEGIN SAMPLE FORM PORTLET-->
                <div class="portlet box blue border-topf">
                    <div class="portlet-body form">
                        <!-- BEGIN FORM-->
                        <form class="horizontal-form" action="#">
                            <div class="row-fluid">
                                <div class="span6">
                                    <div class="control-group">
                                        <h4 class="form-section matop change-password">Email</h4>
                                        <div class="controls">
                                            <input type="text" id="emailId" name="emailid" value="@User.Identity.Name" class="m-wrap span12 border-form-radius" placeholder="populate existing email" />
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row-fluid">
                                <div class="span6">
                                    <h4 class="form-section matop change-password">Change Password</h4>
                                    <div class="control-group">
                                        <div class="controls">
                                            <label class="control-label">Existing password</label>
                                            <input type="password" name="existingpwd" class="m-wrap span12 border-form-radius">
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row-fluid">
                                <div class="span6">
                                    <div class="control-group">
                                        <div class="controls">
                                            <label class="control-label">New password</label>
                                            <input name="newpwd" type="password" class="m-wrap span12 border-form-radius">
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="row-fluid">
                                <div class="span6">
                                    <div class="control-group">
                                        <div class="controls">
                                            <label class="control-label">Confirm password</label>
                                            <input type="password" name="confirmpwd" class="m-wrap span12 border-form-radius">
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="form-actions ">
                                <button class="btn blue" type="submit"><i class="icon-ok"></i>Save</button>
                                <button class="btn" type="button">Cancel</button>
                            </div>
                        </form>
                        <!-- END FORM-->
                    </div>
                </div>
                <!-- END SAMPLE FORM PORTLET-->
            </div>
        </div>

        <!-- END PAGE CONTENT-->
    </div>
    <!-- END PAGE CONTAINER-->
</div>
}

