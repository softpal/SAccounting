@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width">
    <title>Stratus Accounting</title>

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,800,700,300,600,300italic' rel='stylesheet' type='text/css'>
    <link href="../../stratus-home/css/css/style.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="../../stratus-home/css/css/normalize.css" type="text/css" media="screen">
    <link rel="stylesheet" href="../../stratus-home/css/css/grid.css" type="text/css" media="screen">
</head>
<body>
    <div id="wrapper">
        <div class="menu">
            @{
